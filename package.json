{
  "name": "spiders.js",
  "version": "2.0.0",
  "main": "./src/spiders.js",
  "typings": "./index.d.ts",
  "scripts": {
    "build-client-tests": "browserify client-tests/testBaseFramework.js -o client-tests/baseBundle.js",
    "build-full-tests-client": "browserify full-tests/client.js -o full-tests/full-bundle.js",
    "build-client-benchmarks": "browserify benchmarks/Client-Benchmarks/ClientBenchmarks.js -o benchmarks/Client-Benchmarks/bundle.js",
    "build-spiderpong-client": "browserify SpiderPong/spiderClient/js/logic.js -o SpiderPong/spiderClient/bundle.js",
    "build-nativepong-client": "browserify NativePong/nativeClient/js/logic.js -o NativePong/nativeClient/bundle.js",
    "build-speedup-client": "browserify benchmarks/Speedup/ClientSpeedupBenchmarks.js -o benchmarks/Speedup/bundle.js",
    "build-ping-example": "browserify examples/Distributed/pinger.js -o examples/Distributed/pingBundle.js",
    "build-pong-example": "browserify examples/Distributed/ponger.js -o examples/Distributed/pongBundle.js",
    "server-skeleton": "node skeletons/builder.js server"
  },
  "dependencies": {
    "browserify": "14.0.0",
    "bufferutil": "1.3.0",
    "csv-write-stream": "^2.0.0",
    "fast-csv": "^2.4.1",
    "fast-stats": "0.0.3",
    "lz-string": "^1.4.4",
    "net": "1.0.2",
    "plotly": "^1.0.6",
    "socket.io": "1.7.2",
    "socket.io-client": "1.7.2",
    "utf-8-validate": "2.0.0",
    "webworkify": "1.4.0",
    "geocoder": "^0.2.3",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^3.5.0",
    "decimal.js": "7.1.1",
    "mocha": "^3.5.3",
    "spiders.js": "latest"
  }
}
